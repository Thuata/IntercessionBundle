{
    "name": "thuata/intercessionbundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Bundle to help code generation",
    "authors": [
        {
            "name": "Anthony Maudry",
            "email": "anthony.maudry@thuata.com",
            "homepage": "http://www.thuata.com/"
        }
    ],
    "autoload": {
        "psr-4": { "Thuata\\IntercessionBundle\\": "" }
    },
    "require": {
        "php": "~7.1",
        "symfony/symfony": "^3",
        "thuata/componentBundle" : "^1",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3",
        "phpunit/phpunit": "5.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1-dev"
        }
    },
    "repositories" : [
        {
            "type" : "git",
            "url" : "git@github.com:Thuata/ComponentBundle.git"
        }
    ]
}
